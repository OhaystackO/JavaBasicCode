//测试continue和break
public class CirculationContorl{
	public static void main(String[] args){
	//continue
		for(int i = 1; i < 6; ++i){
			if(i%2 == 0){
				continue;
			}
			System.out.println(i);
		}
		System.out.println("--------");
	//break
			for(int i = 1; i < 6; ++i){
			if(i%2 == 0){
				break;
			}
			System.out.println(i);
		}
	}
}
